{
  "name": "haxemo-discord-bot",
  "version": "1.0.0",
  "description": "Haxemo - A comprehensive Discord moderation bot with MongoDB integration",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "deploy": "node deploy-commands.js",
    "dev": "node --watch index.js",
    "install-service": "sudo cp haxemo.service /etc/systemd/system/ && sudo systemctl daemon-reload",
    "logs": "sudo journalctl -u haxemo -f",
    "status": "sudo systemctl status haxemo"
  },
  "dependencies": {
    "discord.js": "^14.14.1",
    "mongoose": "^8.0.3",
    "dotenv": "^16.3.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": ["discord", "bot", "moderation", "mongodb", "nodejs", "discord.js"],
  "author": "Noan/Naikia",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/naikia/haxemo.git"
  },
  "bugs": {
    "url": "https://github.com/naikia/haxemo/issues"
  },
  "homepage": "https://naikia.me/haxemo"
}
